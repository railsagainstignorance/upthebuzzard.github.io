---
---
[
  {% for post in site.documents %}
  {
    "title"    : "{{ post.title }}",
    "categories" : [{% for c in post.categories %}"{{ c }}",{% endfor %} null],
    "tags"     : [{% for tag in post.tags %}"{{ tag }}",{% endfor %} null],
    "href"     : "{{ post.url }}",
    "date"     : {
      "day"   : "{{ post.date | date: "%d" }}",
      "month" : "{{ post.date | date: "%B" }}",
      "year"  : "{{ post.date | date: "%Y" }}"
    }
  },  {% endfor %}
  null
]
